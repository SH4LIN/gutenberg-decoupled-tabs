{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "decoupled-tabs/tabs-selector",
  "version": "1.0.0",
  "title": "Tabs Selector",
  "category": "theme",
  "icon": "list-view",
  "description": "Tabs Selector block",
  "supports": {
    "html": false,
    "multiple": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "color": true
    }
  },
  "keywords": [
    "tabs",
    "decoupled tabs",
    "selector"
  ],
  "attributes": {
    "type": {
      "type": "string",
      "default": "drop-down"
    },
    "tabs": {
      "type": "array",
      "default": []
    },
    "dropDownLabel": {
      "type": "string",
      "default": ""
    },
    "selectedTab": {
      "type": "string",
      "default": ""
    },
    "defaultTab": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "viewportWidth": 789,
    "attributes": {
      "tabs": [
        {
          "oldId": "",
          "id": "USA",
          "name": "United States of America",
          "isEditing": false
        }
      ]
    }
  },
  "textdomain": "decoupled-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}