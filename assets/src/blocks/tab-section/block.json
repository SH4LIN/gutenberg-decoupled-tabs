{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "decoupled-tabs/tab-section",
	"version": "1.0.0",
	"title": "Tab Section",
	"category": "theme",
	"icon": "smiley",
	"description": "Tab Section Block",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"margin": true,
			"padding": true
		},
		"color": {
			"background": true,
			"link": true,
			"text": true,
			"gradients": true
		},
		"layout": true
	},
	"attributes": {
		"defaultTabSection": {
			"type": "string",
			"default": ""
		}
	},
	"example": {},
	"textdomain": "decoupled-tabs",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js"
}
